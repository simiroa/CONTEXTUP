{
    "engine": "Midj",
    "name": "Midjourney V6 Standard",
    "description": "Standard Midjourney prompt template with parameter controls.",
    "inputs": [
        {
            "id": "subject",
            "label": "Subject",
            "default": ""
        },
        {
            "id": "scene",
            "label": "Scene Description",
            "default": ""
        },
        {
            "id": "style",
            "label": "Artistic Style",
            "default": ""
        },
        {
            "id": "mood",
            "label": "Mood/Atmosphere",
            "default": ""
        }
    ],
    "options": [
        {
            "id": "aspect",
            "label": "Aspect Ratio",
            "choices": [
                "--ar 16:9",
                "--ar 1:1",
                "--ar 4:3",
                "--ar 9:16",
                "--ar 3:2",
                "--ar 2:3"
            ],
            "default": "--ar 16:9"
        },
        {
            "id": "stylize",
            "label": "Stylize Level",
            "choices": [
                "--stylize 0",
                "--stylize 50",
                "--stylize 100",
                "--stylize 250",
                "--stylize 500",
                "--stylize 750",
                "--stylize 1000"
            ],
            "default": "--stylize 100"
        },
        {
            "id": "quality",
            "label": "Quality",
            "choices": [
                "",
                "--quality 0.25",
                "--quality 0.5",
                "--quality 2"
            ],
            "default": ""
        }
    ],
    "template": "{subject}, {scene}, {style}, {mood} {aspect} {stylize} {quality} --v 6",
    "example_image": "portrait.png"
}